      }
    ]
  },
  {
    "name": "Room Capture Things",
    "patterns":
    [
      {
        "pattern": "\\((even match|uber|challenging|difficult|easy|trivial)\\) (.+)$",
        "type": "regex"
      }
    ]
  },
  {
    "name": "End Capture Room",
    "patterns":
    [
      {
        "pattern": "^<\\d+-\\d+ \\d+-\\d+ \\d+-\\d+ \\d+   \\d+ \\d+ \\d+ \\d\\d:\\d\\d (?:Ldr:|Fol:)?\\w* >",
        "type": "regex"
      }
    ]
  },
  {
    "name": "Still Fighting",
